    "browserify": "^12.0.0",
    "choppa": "^1.0.2",
    "drag-drop": "^2.11.0",
    "filereader-stream": "^1.0.0",
    "gh-pages-deploy": "^0.4.2",
    "standard": "^3.0.0",
    "watchify": "~3.6.0",
    "wzrd": "^1.3.1"
  },
  "dependencies": {
    "file-tree-browser-widget": "^1.1.0",
    "hyperdrive": "^5.2.0",
    "level-browserify": "^1.1.0",
    "memdb": "^1.3.1",
    "ractive": "^0.7.3",
    "render-data": "^1.0.0",
    "signalhub": "^4.4.0",
    "webrtc-swarm": "^2.4.0",
    "yo-yo": "^1.2.1"
